{
  "name": "fixblox-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "clients/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "npm run dev --workspace apps/fixblox-web",
    "build": "npm run build --workspaces --if-present",
    "build:web": "npm run build --workspace apps/fixblox-web",
    "lint": "npm run lint --workspaces --if-present",
    "lint:web": "npm run lint --workspace apps/fixblox-web"
  }
}
